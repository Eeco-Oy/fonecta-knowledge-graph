{
  "$id": "wp_pageConnector",
  "$schema": "https://schema.yext.com/config/km/connector/v4",
  "displayName": "WordPress Page Connector",
  "entityTypes": [
    "wp_page"
  ],
  "source": {
    "sourceConfig": {
      "nativeSourceConfig": {
        "source": "wordpress",
        "operation": "fetchContent",
        "inputs": {
          "contentType": "pages",
          "wordpressFullDomain": "https://www.aava.fi/laakarikeskukset/aava-helsinki-kamppi/"
        }
      }
    },
    "selectors": [
      {
        "type": "JSON",
        "header": "Entity ID",
        "path": "id"
      },
      {
        "type": "JSON",
        "header": "Name",
        "path": "title.rendered"
      },
      {
        "type": "JSON",
        "header": "Date Posted (raw)",
        "path": "date"
      },
      {
        "type": "JSON",
        "header": "Date Modified (raw)",
        "path": "modified"
      },
      {
        "type": "JSON",
        "header": "Author",
        "path": "_embedded.author[].name"
      },
      {
        "type": "JSON",
        "header": "Author Landing Page",
        "path": "_embedded.author[].link"
      },
      {
        "type": "JSON",
        "header": "Author Description",
        "path": "_embedded.author[].description"
      },
      {
        "type": "JSON",
        "header": "Landing Page URL",
        "path": "link"
      }
    ]
  },
  "transforms": [
    {
      "extractText": {
        "inputHeader": "Date Posted (raw)",
        "newColumnHeader": "Date Posted",
        "extractOperation": "ALL_TEXT_BEFORE",
        "extractFrom": "FIRST_INSTANCE_MATCHING_TEXT",
        "valueToFind": {
          "plainText": {
            "value": "T",
            "ignoreCase": false
          }
        }
      }
    },
    {
      "extractText": {
        "inputHeader": "Date Modified (raw)",
        "newColumnHeader": "Date Modified",
        "extractOperation": "ALL_TEXT_BEFORE",
        "extractFrom": "FIRST_INSTANCE_MATCHING_TEXT",
        "valueToFind": {
          "plainText": {
            "value": "T",
            "ignoreCase": false
          }
        }
      }
    }
  ],
  "mappings": [
    {
      "header": "Landing Page URL",
      "field": "landingPageUrl",
      "subfieldPath": [],
      "clearValueIfBlank": false
    },
    {
      "header": "Entity ID",
      "field": "entityId",
      "subfieldPath": [],
      "clearValueIfBlank": false
    },
    {
      "header": "Author Landing Page",
      "field": "wp_authorLandingPage",
      "subfieldPath": [],
      "clearValueIfBlank": false
    },
    {
      "header": "Author Description",
      "field": "wp_authorDescription",
      "subfieldPath": [],
      "clearValueIfBlank": false
    },
    {
      "header": "Date Posted",
      "field": "wp_datePosted",
      "subfieldPath": [],
      "clearValueIfBlank": false
    },
    {
      "header": "Author",
      "field": "wp_author",
      "subfieldPath": [],
      "clearValueIfBlank": false
    },
    {
      "header": "Date Modified",
      "field": "wp_dateModified",
      "subfieldPath": [],
      "clearValueIfBlank": false
    },
    {
      "header": "Name",
      "field": "name",
      "subfieldPath": [],
      "clearValueIfBlank": false
    }
  ],
  "scheduleConfig": {
    "customSchedule": {
      "timeZone": "America/New_York",
      "startDate": "2022-01-31",
      "startTime": "00:00",
      "frequencyType": "DAILY"
    },
    "runMode": "COMPREHENSIVE",
    "dryRun": false
  },
  "comprehensiveModeConfig": {
    "comprehensiveSet": "ENTITIES_CREATED_BY_THIS_CONNECTOR",
    "actionOnMissingEntities": "DELETE"
  }
}